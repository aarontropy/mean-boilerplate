{
    "site_js": {
        "public/build/site_js": [
            "public/lib/angular/angular.js",
            "public/js/app.js",
            "public/js/controllers/**/*.js",
            "public/js/services/**/*.js"
        ]
    },
    "admin_js": {
        "public/build/admin_vendor.js": [
            "public/lib/jquery/dist/jquery.js",
            "public/lib/bootstrap/dist/js/bootstrap.js",
            "public/lib/select2/select2.js",
            "public/lib/tinymce/js/tinymce/tinymce.js"
        ],
        "public/build/admin_angular.js" : [
            "public/lib/angular/angular.js",
            "public/lib/angular-ui-router/release/angular-ui-router.js",
            "public/lib/angular-resource/angular-resource.js",
            "public/lib/angular-ui-tinymce/src/tinymce.js",
            "public/lib/angular-ui-select2/src/select2.js",
            "public/lib/angular-ui-bootstrap/dist/ui-bootstrap-tpls-0.10.0.js"
        ]
    },
    "admin_app": {
        "public/build/admin_app.js": [
            "public/admin/app.js",
            "public/admin/controllers/**/*.js",
            "public/admin/services/**/*.js"
